package threads;

public class Main {

	public static void main(String[] args) {
		
		Thread thread = new Thread(new Runnable(){
			@Override
			public void run(){
				System.out.println("Hello thread");
			}
		});
		thread.start(); //Inicia a thread
	}
	
	System.out.println("Estudo sobre programação paralela com threads");

}
